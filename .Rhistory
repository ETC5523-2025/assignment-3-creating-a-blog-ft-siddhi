#| label: fig-breach-prob
#| fig-cap: "Distribution of estimated breach probabilities across coverage levels for each vaccine efficacy scenario."
library(ggplot2)
coverage <- seq(0, 90, 10)
eff <- c(60, 75, 90)
df <- expand.grid(Coverage = coverage, Efficacy = eff)
df$Breach <- exp(-0.05 * df$Coverage * (df$Efficacy / 100)) * 0.3
ggplot(df, aes(x = Breach, fill = factor(Efficacy), colour = factor(Efficacy))) +
geom_density(alpha = 0.25, adjust = 1.2, linewidth = 1) +
labs(
x = "Estimated breach probability",
y = "Density",
fill = "Vaccine efficacy (%)",
colour = "Vaccine efficacy (%)"
) +
theme_minimal(base_size = 13)
