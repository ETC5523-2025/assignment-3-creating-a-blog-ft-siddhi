#| label: fig-breach-prob
#| fig-cap: "Interactive breach probability plot built directly with Plotly."
library(plotly)
coverage <- seq(0, 90, 10)
efficacy_levels <- c(60, 75, 90)
df <- expand.grid(Coverage = coverage, Efficacy = efficacy_levels)
df$Breach <- exp(-0.05 * df$Coverage * (df$Efficacy / 100)) * 0.3
plot_ly(
df,
x = ~Coverage,
y = ~Breach,
color = ~factor(Efficacy),
type = 'scatter',
mode = 'lines',
line = list(width = 3)
) %>%
layout(
xaxis = list(title = "Vaccine coverage (%)"),
yaxis = list(title = "Estimated breach probability"),
legend = list(title = list(text = "Vaccine efficacy (%)"))
)
